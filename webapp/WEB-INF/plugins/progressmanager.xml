<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>progressmanager</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.3-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>progressmanager.plugin.description</description>
    <provider>progressmanager.plugin.provider</provider>
    <provider-url>https://fr.lutece.paris.fr/fr/</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>2020</copyright>
    <db-pool-required>1</db-pool-required>
    
   <javascript-files>
        <javascript-files-scope>portal</javascript-files-scope>
        <javascript-file>progressmanager/progress-manager.js</javascript-file>
    </javascript-files>
    
    <freemarker-macro-files>
    	<freemarker-macro-file>admin/plugins/progressmanager/commons_progressmanager.html</freemarker-macro-file>
    </freemarker-macro-files>
    
     <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>PROGRESSMANAGER_MANAGEMENT</feature-id>
            <feature-title>progressmanager.adminFeature.ManageProgressFeeds.name
            </feature-title>
            <feature-description>progressmanager.adminFeature.ManageProgressFeeds.description
            </feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/progressmanager/ManageProgressFeeds.jsp
            </feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>


</plug-in>